    <listOfRules>
      <assignmentRule variable="G_b">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <ci> G_PL_0 </ci>
        </math>
      </assignmentRule>
      <assignmentRule variable="V_G">
