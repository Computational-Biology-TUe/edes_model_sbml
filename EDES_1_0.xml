            <otherwise>
              <apply>
                <times/>
                <ci> sigma </ci>
                <apply>
                  <power/>
                  <ci> k01 </ci>
                  <ci> sigma </ci>
                </apply>
                <apply>
                  <power/>
                  <apply>
                    <minus/>
                    <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> time </csymbol>
                    <ci> Tmeal </ci>
                  </apply>
                  <apply>
                    <minus/>
                    <ci> sigma </ci>
                    <cn> 1 </cn>
                  </apply>
                </apply>
                <apply>
                  <exp/>
                  <apply>
                    <minus/>
                    <apply>
                      <power/>
                      <apply>
                        <times/>
                        <ci> k01 </ci>
                        <apply>
                          <minus/>
                          <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> time </csymbol>
                          <ci> Tmeal </ci>
                        </apply>
                      </apply>
                      <ci> sigma </ci>
                    </apply>
                  </apply>
                </apply>
                <ci> D_meal_G </ci>
              </apply>
            </otherwise>
          </piecewise>
        </math>
      </assignmentRule>
      <assignmentRule variable="I_PL_b">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <ci> I_PL_0 </ci>
        </math>
      </assignmentRule>
      <assignmentRule variable="U_id">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <ci> k05 </ci>
            <ci> f_I </ci>
            <ci> I_d1 </ci>
            <apply>
              <divide/>
              <ci> G_PL </ci>
