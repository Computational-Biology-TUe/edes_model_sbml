            <times/>
            <ci> I_PL </ci>
            <cn> 6.9444 </cn>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="Iprop">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
